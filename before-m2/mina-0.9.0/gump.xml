<?xml version="1.0" encoding="UTF-8"?>

<module name="mina">
  <description>A Multipurpose Infrastrusture for Network Applications</description>
  <url href="http://directory.apache.org/">
  </url>
  <cvs repository="directory">
  </cvs>
  <project name="mina">
    <maven goal="jar">
      <property name="final.name" value="mina-@@DATE@@">
      </property>
    </maven>
    <package>org.apache.mina</package>
    <depend project="commons-net">
    </depend>
    <depend project="emma">
    </depend>
    <depend project="jakarta-ant">
    </depend>
    <depend project="junit">
    </depend>
    <depend project="xml-xerces">
    </depend>
    <work nested="target/classes">
    </work>
    <home nested="target">
    </home>
    <jar name="mina-@@DATE@@.jar">
    </jar>
    <javadoc nested="target/docs/apidocs">
    </javadoc>
    <nag to="dev@directory.apache.org" from="mina development &lt;dev@directory.apache.org&gt;">
    </nag>
  </project>
</module>